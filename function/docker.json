      {
        "name": "JVM_XMX",
        "value": "256m"
      }
    ],
    "labels": [
      {
        "name": "restart",
        "value": "always"
      }
    ],
    "cmd": "",
    "networks": "bridge",
    "mounts": "local"
  },
