      {
        "name": "MINIO_ROOT_PASSWORD",
        "value": "minio"
      }
    ],
    "labels": [
      {
        "name": "restart",
        "value": "always"
      }
    ],
    "cmd": "server /data --console-address :9001",
    "networks": "bridge",
    "mounts": "local"
  },
