      {
        "type": "bind",
        "source": "/home/nacos/conf",
        "destination": "/data/nacos/conf",
        "cp": "/data/nacos/"
      },
      {
        "type": "bind",
        "source": "/home/nacos/data",
        "destination": "/data/nacos/data",
        "cp": "/data/nacos/"
